<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".seller">

    <ImageView
        android:id="@+id/imageView32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/background" />

    <ImageView
        android:id="@+id/gifticon"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="-50dp"
        app:layout_constraintStart_toStartOf="@+id/freepointupcov"
        app:layout_constraintTop_toTopOf="@+id/freepointupcov"
        app:srcCompat="@drawable/gift" />

    <ImageView
        android:id="@+id/freepointupcov"
        android:layout_width="350dp"
        android:layout_height="340dp"
        android:layout_marginTop="75dp"
        android:layout_marginBottom="25dp"
        app:layout_constraintBottom_toTopOf="@+id/proshop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView32"
        app:layout_constraintVertical_bias="0.967"
        app:srcCompat="@drawable/freepointup" />

    <ImageView
        android:id="@+id/adpointupbt"
        android:layout_width="170dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/adpointupbt2"
        app:layout_constraintEnd_toEndOf="@+id/adpointupbt2"
        app:layout_constraintStart_toStartOf="@+id/adpointupbt2"
        app:layout_constraintTop_toTopOf="@+id/adpointupbt2"
        app:srcCompat="@drawable/adbt" />

    <ImageView
        android:id="@+id/adpointupbt2"
        android:layout_width="170dp"
        android:layout_height="50dp"
        android:layout_marginTop="150dp"
        android:layout_marginBottom="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/freepointupcov"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/freepointupcov"
        app:layout_constraintVertical_bias="0.85"
        app:srcCompat="@drawable/adbt" />

    <ImageView
        android:id="@+id/proshop"
        android:layout_width="340dp"
        android:layout_height="180dp"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/freepointupcov"
        app:srcCompat="@drawable/progray" />

    <ImageView
        android:id="@+id/probt"
        android:layout_width="140dp"
        android:layout_height="40dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/proshop"
        app:layout_constraintEnd_toEndOf="@+id/proshop"
        app:srcCompat="@drawable/googleplay" />

    <ImageView
        android:id="@+id/probt2"
        android:layout_width="140dp"
        android:layout_height="40dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/proshop"
        app:layout_constraintEnd_toEndOf="@+id/proshop"
        app:srcCompat="@drawable/googleplay" />

    <ImageView
        android:id="@+id/sellerback"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="15dp"
        android:scaleType="centerInside"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/closebt" />

    <ImageView
        android:id="@+id/sellercount"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bulletgray" />

    <TextView
        android:id="@+id/sellerpoint"
        android:layout_width="28dp"
        android:layout_height="30dp"
        android:gravity="center"
        android:textColor="#FFFFFF"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/sellercount"
        app:layout_constraintEnd_toEndOf="@+id/sellercount"
        app:layout_constraintHorizontal_bias="0.56"
        app:layout_constraintStart_toStartOf="@+id/sellercount"
        app:layout_constraintTop_toTopOf="@+id/sellercount" />

</androidx.constraintlayout.widget.ConstraintLayout>