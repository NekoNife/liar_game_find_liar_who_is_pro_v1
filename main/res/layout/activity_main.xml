<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/contrxt"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">



    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/home" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="153dp"
        android:layout_height="29dp"
        android:gravity="center"
        android:text="라이어를 찾아라!"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.698" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="133dp"
        android:layout_height="54dp"
        android:fontFamily="@font/sungdong"
        android:gravity="center"
        android:text="Start"
        android:textColor="#FFFFFF"
        android:textSize="70sp"
        app:layout_constraintBottom_toTopOf="@+id/fristhelp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/startbt"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/setbt"
        app:srcCompat="@drawable/tm" />

    <ImageView
        android:id="@+id/setbt"
        android:layout_width="40dp"
        android:layout_height="41dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/i_button" />

    <TextView
        android:id="@+id/fristhelp"
        android:layout_width="208dp"
        android:layout_height="33dp"
        android:layout_marginBottom="40dp"
        android:gravity="center"
        android:text="화면을 눌러 게임을 시작하세요"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView5"
        app:layout_constraintVertical_bias="0.939" />

    <ImageView
        android:id="@+id/adcov2"
        android:layout_width="382dp"
        android:layout_height="351dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/frame_subj" />

    <ImageView
        android:id="@+id/adyes3"
        android:layout_width="140dp"
        android:layout_height="40dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="50dp"
        android:foreground="@drawable/drawbackgrape"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/adno3"
        app:layout_constraintEnd_toEndOf="@+id/adcov2"
        app:layout_constraintStart_toEndOf="@+id/adno3"
        app:layout_constraintTop_toTopOf="@+id/adno3"
        app:srcCompat="@drawable/none" />

    <TextView
        android:id="@+id/adyes4"
        android:layout_width="140dp"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="광고보기"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/adyes3"
        app:layout_constraintEnd_toEndOf="@+id/adyes3"
        app:layout_constraintStart_toStartOf="@+id/adyes3"
        app:layout_constraintTop_toTopOf="@+id/adyes3" />

    <ImageView
        android:id="@+id/adno3"
        android:layout_width="140dp"
        android:layout_height="40dp"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="25dp"
        android:foreground="@drawable/drawbackgrape"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toStartOf="@+id/adyes3"
        app:layout_constraintStart_toStartOf="@+id/adcov2"
        app:layout_constraintTop_toBottomOf="@+id/admo2"
        app:srcCompat="@drawable/none" />

    <TextView
        android:id="@+id/adno4"
        android:layout_width="140dp"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="취소"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/adno3"
        app:layout_constraintEnd_toEndOf="@+id/adno3"
        app:layout_constraintStart_toStartOf="@+id/adno3"
        app:layout_constraintTop_toTopOf="@+id/adno3" />

    <TextView
        android:id="@+id/admo2"
        android:layout_width="304dp"
        android:layout_height="168dp"
        android:fontFamily="sans-serif"
        android:gravity="center"
        android:text="광고제거 횟수를 모두\n사용하셨습니다."
        android:textColor="#FFFFFF"
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="@+id/adcov2"
        app:layout_constraintStart_toStartOf="@+id/adcov2"
        app:layout_constraintTop_toTopOf="@+id/adcov2"
        app:layout_constraintVertical_bias="0.378" />


</androidx.constraintlayout.widget.ConstraintLayout>